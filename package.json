	"description": "Light-weight Fullstack PHP MVC and ReactJS!",
	"main": "app.js",
	"scripts": {
		"start": "concurrently \"npm run dev\" \"npm run server\"",
		"dev": "SET NODE_ENV=development && webpack --mode development --hide-modules",
		"build": "SET NODE_ENV=production && webpack --mode production --hide-modules",
		"w-build": "SET NODE_ENV=production && webpack --watch --mode production --hide-modules",
		"server": "nodemon server.js"
	},
